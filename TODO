    (use 'clojure.contrib.monads)
    
    (defn generate-one-element-at-a-time [list]
      (def control-state
        (atom
         (fn [return]
           (let [return (atom return)]
             (doseq [element list]
               (swap!
                return
                (fn [return]
                  (call-cc
                   (fn [resume-here]
                     (swap! control-state
                            (fn [control-state] resume-here))
                     (return element)))))))
           (return 'fuck-this-shit))))
      (defn generator []
        (call-cc @control-state))
      generator)
    
