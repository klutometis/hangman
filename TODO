    (defn map-trie
      ([map trie predicates]
         (map-trie map trie predicates '() '()))
      ([map trie predicates words word]
         ;; (debug trie words word)
         (if (trie-sentinel? trie)
           (let [word (reverse word)]
             (if (every-predicate? predicates word)
               (list (map word))
               nil))
